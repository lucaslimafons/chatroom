{
  "name": "chatroom",
  "version": "1.0.0",
  "description": "chatroom for jobsity",
  "private": true,
  "scripts": {
    "start": "nodemon ./bin/www",
    "test": "NODE_ENV=test mocha",
    "watch": "sass --watch ./assets/scss:public/stylesheets",
    "pre-migrate": "node pre-migrate.js",
    "migrate": "NODE_ENV=$1 && npm run pre-migrate && sequelize db:migrate --env $1",
    "undo_migrate": "sequelize db:migrate:undo --env $1"
  },
  "dependencies": {
    "bcrypt-nodejs": "0.0.3",
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.5",
    "debug": "~2.6.9",
    "dotenv": "^8.2.0",
    "express": "~4.16.1",
    "express-session": "^1.17.1",
    "fast-csv": "^4.3.1",
    "http-errors": "~1.6.3",
    "method-override": "^3.0.0",
    "moment": "^2.27.0",
    "morgan": "~1.9.1",
    "mysql2": "^2.1.0",
    "node-sass": "^4.14.1",
    "nodemon": "^2.0.4",
    "passport": "^0.4.1",
    "passport-http-bearer": "^1.0.1",
    "path": "^0.12.7",
    "request-promise": "^4.2.6",
    "sequelize": "^6.3.4",
    "sequelize-cli": "^6.2.0",
    "socket.io": "^2.3.0"
  }
}
